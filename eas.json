    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "simulator": true 
      },
      "channel": "preview"
    },
    "beta": {
      "channel": "beta"
    },
    "prod": {
      "channel": "prod"
    }
  },
  "submit": {
    "prod": {}
  }
}
